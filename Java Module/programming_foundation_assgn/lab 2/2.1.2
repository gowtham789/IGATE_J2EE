BEGIN

   DECLARE    EXP AS INTEGER //employee experience 
   PROMPT     "Enter the experience of employee:"
   ACCEPT     EXP
   
   IF ( EXP== 0 || EXP<3)THEN
		 PRINT "The tag color is blue"
		 
	ELSE IF ( EXp==3 || EXP<5 ) THEN
		 PRINT  "The tag color is grey."
		 
	ELSE IF ( EXp==5 || EXP<10 ) THEN
		 PRINT  "The tag color is Yellow."	
		 
    ELSE
         PRINT  "The tag color is red."		
	END IF
	
END
