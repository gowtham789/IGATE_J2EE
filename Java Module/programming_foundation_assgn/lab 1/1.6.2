BEGIN 

  DECLARE num[10] AS INTEGER ARRAY

  FOR index=1 to 10
  
       ACCEPT num[index]
  
  END FOR
  
  FOR index=1 to 10
     IF(num[index]%2==0) THEN
	     
		 PRINT "Even numbers are" +num[index]
		 index=index+1
     END IF
  END FOR
  
   FOR index=1 to 10
     IF(num[index]%2!=0) THEN
	     
		 PRINT "Odd numbers are" +num[index]
		 index=index+1
     END IF
  END FOR
  
 END
	     
  
  
 