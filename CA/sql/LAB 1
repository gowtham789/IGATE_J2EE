1. SELECT STAFF_NAME NAME, STAFF_sAL SALARY, DEPT_CODE CODE
	FROM STAFF_MASTER
	WHERE CODE=(10,20,30);
	
2.  SELECT STUDENT_CODE, SUBJECT1, SUBJECT2, SUBJECT3, 
	(SUBJECT1+SUBJECT2+SUBJECT3) AS "TOTAL"
	FROM STUDENT_MARKS
	/

3. SELECT STAFF_NAME NAME, DESIGN_CODE DESIGNATION
	FROM STAFF_MASTER
	WHERE (HIREDATE<'01-JAN-03') AND (STAFF_SAL BETWEEN 12000 AND 25000);
	
4. SELECT STAFF_NAME, STAFF_CODE, DEPT_CODE, HIREDATE, FLOOR((SYSDATE-HIREDATE)/365) EXPERIENCE
	FROM STAFF_MASTER
	WHERE SYSDATE-HIREDATE>18
	ORDER BY HIREDATE;

5. SELECT STAFF_NAME, DEPT_CODE, STAFF_SAL, STAFF_SAL*10 "10 YEAR GROSS"
	FROM STAFF_MASTER
	WHERE DEPT_CODE=10 OR DEPT_CODE=30;
	
6. SELECT STAFF_NAME||', '||DEPT_CODE "STUDENT INFO"                 LAB1
	FROM STAFF_MASTER

7. SELECT *
	FROM STAFF_MASTER
	WHERE MGR_CODE IS NULL;
	
8. SELECT STUDENT_NAME NAME, DEPT_CODE DCODE, STUDENT_DOB DOB
	FROM STUDENT_MASTER
	WHERE STUDENT_DOB BETWEEN '01-JAN-81' AND '31-MAR-83'
	ORDER BY STUDENT_DOB;
	
9. SELECT *FROM BOOK_MASTER
    WHERE BOOK_PUB_YEAR BETWEEN 2001 AND 2004;
	 SELECT STAFF_NAME FROM STAFF_MASTER
	 WHERE STAFF_NAME LIKE '%\&%';
	 
10. SELECT *FROM BOOK_MASTER
	 WHERE UPPER(BOOK_NAME) LIKE '%COMP%';
	 
11. SELECT * FROM STAFF_MASTER
	 WHERE UPPER(STAFF_NAME) LIKE 'A%S'
	 OR (UPPER(STAFF_NAME) LIKE '_N%' OR UPPER(STAFF_NAME) LIKE '__N')
	 AND (UPPER(STAFF_NAME) LIKE '%S' OR UPPER(STAFF_NAME) LIKE '%N');
	 
12. SELECT STAFF_NAME FROM STAFF_MASTER
	 WHERE STAFF_NAME LIKE '%\_%';
	

IGATE Sensitive

